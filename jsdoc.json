{
  "source": {
    "include": ["app", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "collapse": true,
    "referenceTitle": "Brunch crank skeleton"
  }
}
